{
  "name": "todo-app",
  "version": "1.0.0",
  "main": "index.html",
  "scripts": {
    "start": "parcel index.html --open",
    "minify-css": "uglifycss src/todo-app.css --output build/todo-app.min.css",
    "minify-js": "uglifyjs src/todo-app.js -o build/todo-app.min.js",
    "copy-html": "copyfiles index.html build",
    "build": "npm-run-all copy-html minify-js minify-css"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.14.4",
    "copyfiles": "^2.4.1",
    "npm-run-all": "^4.1.5",
    "uglify-js": "^3.13.8",
    "uglifycss": "0.0.29"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "keywords": [
    "javascript",
    "starter"
  ]
}
